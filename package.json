{
  "name": "testwebpack",
  "version": "1.0.0",
  "description": "trying out webpack",
  "repository": "github.com:dankennedy/testwebpack",
  "main": "lib/server/server.js",
  "scripts": {
    "cleanbuild": "rm -f ./public/*.*",
    "lint": "eslint .",
    "prebuild": "npm run lint && npm run cleanbuild",
    "predev": "npm run lint && npm run cleanbuild",
    "pretest": "npm run lint",
    "pretestwatch": "npm run lint",
    "start": "NODE_PATH=lib NODE_ENV=production node lib/server/server.js",
    "dev": "NODE_PATH=lib NODE_ENV=development node lib/server/server.js",
    "test": "NODE_PATH=lib NODE_ENV=development mocha --compilers js:babel/register -u tdd --timeout 5000",
    "testwatch": "NODE_PATH=lib NODE_ENV=development mocha --compilers js:babel/register -u tdd --reporter min --watch --timeout 5000",
    "build": "NODE_PATH=lib NODE_ENV=production webpack -p -d --progress --colors --config=./webpack.prod.config.js"
  },
  "author": "dankennedy",
  "license": "MIT",
  "readme": "README.md",
  "devDependencies": {
    "babel": "^5.5.8",
    "babel-core": "^5.5.8",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.1.4",
    "babel-runtime": "^6.0.14",
    "blueimp-tmpl": "^2.5.5",
    "css-loader": "^0.14.5",
    "eslint": "^1.5.1",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.4.2",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.6.2",
    "http-proxy": "^1.11.1",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.2.0",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "sass-loader": "^1.0.2",
    "style-loader": "^0.12.3",
    "supertest": "^1.1.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  },
  "dependencies": {
    "axios": "^0.7.0",
    "bluebird": "^3.0.0",
    "body-parser": "^1.14.1",
    "bunyan": "^1.4.0",
    "bunyan-prettystream": "^0.1.3",
    "express": "^4.13.3",
    "express-bunyan-logger": "^1.1.1",
    "express-handlebars": "^2.0.1",
    "lodash": "^3.9.3",
    "moment": "^2.10.3",
    "mongoose": "^4.2.2",
    "nodemailer": "^1.8.0",
    "react-date-picker": "^2.1.6",
    "react-notify-toast": "0.0.3",
    "react-swipeable": "^3.0.2"
  }
}
